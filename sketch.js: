function setup() {
  createCanvas(900, 900);
}

function draw() {
  background(220);
}function setup() {
  createCanvas(900, 900);
  background("rgb(36,35,35)")
}

function draw() {
  stroke("blue");
  fill("red");
  
  
  if (mouseIsPressed) {
    rect(mouseX, mouseY, 20, 35);
  }
}